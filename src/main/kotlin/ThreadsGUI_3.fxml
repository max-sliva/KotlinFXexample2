<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="395.0" prefWidth="596.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ThreadsGUIController2">
   <center>
      <VBox>
         <children>
            <HBox prefHeight="135.0" prefWidth="596.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox alignment="CENTER" style="-fx-border-color: orange; -fx-border-width: 6;" HBox.hgrow="ALWAYS">
                     <children>
                        <Region prefHeight="200.0" prefWidth="200.0" />
                        <Button mnemonicParsing="false" onAction="#handleOnStartA" text="StartA" />
                        <Region prefHeight="200.0" prefWidth="200.0" />
                        <Button mnemonicParsing="false" onAction="#handleOnPauseA" text="PauseA" />
                        <Region prefHeight="200.0" prefWidth="200.0" />
                        <Button mnemonicParsing="false" onAction="#handleOnStopA" text="StopA" />
                        <Region prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </VBox>
                  <Region HBox.hgrow="ALWAYS" />
                  <VBox alignment="CENTER" style="-fx-border-color: olive; -fx-border-width: 6;" HBox.hgrow="ALWAYS">
                     <children>
                        <Region prefHeight="200.0" prefWidth="200.0" />
                        <Button mnemonicParsing="false" onAction="#handleOnStartB" text="StartB" />
                        <Region prefHeight="200.0" prefWidth="200.0" />
                        <Button mnemonicParsing="false" text="PauseB" />
                        <Region prefHeight="200.0" prefWidth="200.0" />
                        <Button mnemonicParsing="false" onAction="#handleOnStopB" text="StopB" />
                        <Region prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <TextArea fx:id="textArea" prefHeight="224.0" prefWidth="596.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </center>
</BorderPane>
