<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="accountTable" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AccountController">
   <columns>
      <TableColumn fx:id="loginCol" prefWidth="75.0" text="Логин" />
      <TableColumn fx:id="fioCol" prefWidth="75.0" text="ФИО" />
      <TableColumn fx:id="statusCol" prefWidth="75.0" text="Статус" />
      <TableColumn fx:id="passCol" prefWidth="75.0" text="Пароль" />
   </columns>
   <columnResizePolicy>
      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
   </columnResizePolicy>
</TableView>
